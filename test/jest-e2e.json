{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "modulePaths": ["<rootDir>/libs/","<rootDir>/src/"],
  "moduleNameMapper": {
    "^@app/snowflake/(.*)$": "<rootDir>/libs/Snowflake/src/$1",
    "@app/snowflake": "<rootDir>/libs/Snowflake/src",
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}